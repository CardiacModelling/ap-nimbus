az aks create --resource-group <resource-group-id> --name learnAKSCluster --node-count 1 --enable-addons monitoring --generate-ssh-keys
az aks get-credentials --resource-group <resource-group-id> --name learnAKSCluster
az aks delete --resource-group <resource-group-id> --name learnAKSCluster
